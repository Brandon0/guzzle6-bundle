{
    "name": "e-moe/guzzle-bundle",
    "type": "symfony-bundle",
    "description": "Integrates Guzzle into your Symfony2 application",
    "keywords": ["guzzle", "bundle", "http", "http client", "rest", "client", "web service", "curl", "api"],
    "homepage": "https://github.com/e-moe/xxx",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Labinskiy aka e-moe",
            "email": "e-moe@ukr.net"
        }
    ],
    "support": {
        "issues": "https://github.com/e-moe/xxx/issues"
    },
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~2.7",
        "guzzle/guzzle": "~6.0"
    },
    "suggest": {
        "symfony/monolog-bundle": "Log requests"
    },
    "conflict": {
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "symfony/monolog-bundle": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Emoe\\GuzzleBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
